<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pranav.helloandroid">
    <uses-sdk android:minSdkVersion="21" android:targetSdkVersion="27" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IssueNavigation"></activity>
        <activity android:name=".MoreProjectInfo"></activity>
        <activity android:name=".ProjectTime"></activity>
        <activity android:name=".ProjectLocation"></activity>
        <activity android:name=".CustomerInfo"></activity>
        <activity android:name=".WorkSummaryActivity"></activity>
    </application>
</manifest>